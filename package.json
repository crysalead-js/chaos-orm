{
  "name": "chaos-orm",
  "version": "8.0.0",
  "description": "Datastore Agnostic Model Layer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/crysalead-js/chaos-orm.git"
  },
  "keywords": [
    "orm"
  ],
  "scripts": {
    "compile": "BABEL_ENV=production babel -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "mocha --compilers babel:babel-core/register",
    "coverage-report": "babel-node node_modules/isparta/bin/isparta cover _mocha --i \"src/**/*.js\"",
    "push-coverage": "cat ./coverage/lcov.info | coveralls",
    "cleanup": "rm -rf ./coverage"
  },
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "dependencies": {
    "camel-case": "^3.0.0",
    "chaos-validator": "^2.0",
    "co": "^4.6.0",
    "component-emitter": "^1.2.1",
    "dateformat-light": "^1.0.14",
    "dotpath-parser": "0.0.2",
    "expand-flatten": "^1.0.0",
    "extend-merge": "^1.0.3",
    "intersect": "^1.0.1",
    "node-uuid": "^1.4.7",
    "pascal-case": "^2.0.0",
    "pluralize": "^3.0.0",
    "snake-case": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "bluebird": "^3.4.1",
    "chai-kahlan": "^1.0.4",
    "coveralls": "^2.11.9",
    "isparta": "^4.0.0",
    "jasmine-kahlan": "^1.0.2",
    "mocha": "^3.0.1"
  },
  "babel": {
    "env": {
      "development": {
        "plugins": ["transform-es2015-modules-commonjs"]
      },
      "production": {
        "presets": ["es2015"]
      }
    }
  }
}
